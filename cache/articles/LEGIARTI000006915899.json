                {
                    "textCid": "JORFTEXT000030711300",
                    "textTitle": "D\u00c9CRET n\u00b02015-647 du 10 juin 2015 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000030712304",
                    "natureText": "DECRET",
                    "datePubliTexte": "2015-06-12",
                    "dateSignaTexte": "2015-06-10",
