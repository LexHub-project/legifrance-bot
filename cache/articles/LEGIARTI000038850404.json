                {
                    "textCid": "JORFTEXT000038812273",
                    "textTitle": "D\u00e9cret n\u00b02019-772 du 24 juillet 2019 - art.",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "",
