                {
                    "textCid": "JORFTEXT000030559751",
                    "textTitle": "D\u00c9CRET n\u00b02015-513 du 7 mai 2015 - art. 14",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "14",
                    "articleId": "LEGIARTI000030561285",
                    "natureText": "DECRET",
