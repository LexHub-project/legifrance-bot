                {
                    "textCid": "JORFTEXT000034379166",
                    "textTitle": "Ordonnance n\u00b02017-484 du 6 avril 2017 - art. 12",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "12",
