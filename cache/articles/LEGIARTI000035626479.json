                {
                    "textCid": "JORFTEXT000035607348",
                    "textTitle": "Ordonnance n\u00b02017-1386 du 22 septembre 2017 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
