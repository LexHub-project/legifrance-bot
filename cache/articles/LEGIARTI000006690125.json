                {
                    "textCid": "JORFTEXT000025015748",
                    "textTitle": "Ordonnance n\u00b02011-1922\n du 22 d\u00e9cembre 2011 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
