                {
                    "textCid": "JORFTEXT000035720869",
                    "textTitle": "Ordonnance n\u00b02017-1433 du 4 octobre 2017 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000035721922",
                    "natureText": "ORDONNANCE",
