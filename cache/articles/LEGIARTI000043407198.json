                    "textCid": "JORFTEXT000044176983",
                    "textTitle": "LOI n\u00b02021-1308 du 8 octobre 2021 - art. 27 (V)",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "27",
                    "articleId": "LEGIARTI000044180515",
