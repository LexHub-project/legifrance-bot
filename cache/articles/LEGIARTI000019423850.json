            "lienModifications": [
                {
                    "textCid": "JORFTEXT000034270980",
                    "textTitle": "D\u00e9cret n\u00b02017-389 du 23 mars 2017 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
