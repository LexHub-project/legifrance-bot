                {
                    "textCid": "JORFTEXT000025372075",
                    "textTitle": "D\u00e9cret n\u00b02012-219\n du 16 f\u00e9vrier 2012 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000025373259",
                    "natureText": "DECRET",
