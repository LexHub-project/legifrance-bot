                {
                    "textCid": "JORFTEXT000000698423",
                    "textTitle": "D\u00e9cret 89-111 1989-02-21 art. 2, art. 3 JORF 23 f\u00e9vrier 1989",
                    "linkType": "MODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "",
                    "articleId": "JORFTEXT000000698423",
                    "natureText": "DECRET",
