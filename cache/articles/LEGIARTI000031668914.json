                {
                    "textCid": "JORFTEXT000037847585",
                    "textTitle": "LOI n\u00b02018-1203 du 22 d\u00e9cembre 2018 - art. 52 (V)",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "52",
                    "articleId": "LEGIARTI000037849648",
                    "natureText": "LOI",
                    "datePubliTexte": "2018-12-23",
