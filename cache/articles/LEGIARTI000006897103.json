                {
                    "textCid": "JORFTEXT000000821978",
                    "textTitle": "D\u00e9cret n\u00b02007-535 du 10 avril 2007 - art. 1 () JORF 11 avril 2007",
                    "linkType": "MODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "1",
