                {
                    "textCid": "JORFTEXT000037192797",
                    "textTitle": "LOI n\u00b02018-607 du 13 juillet 2018 - art. 37",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "37",
                    "articleId": "LEGIARTI000037194669",
                    "natureText": "LOI",
