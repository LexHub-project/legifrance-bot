                {
                    "textCid": "JORFTEXT000032367437",
                    "textTitle": "D\u00e9cret n\u00b02016-401 du 5 avril 2016 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
