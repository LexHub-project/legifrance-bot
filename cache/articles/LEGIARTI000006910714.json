                {
                    "textCid": "JORFTEXT000039684551",
                    "textTitle": "D\u00e9cret n\u00b02019-1483 du 27 d\u00e9cembre 2019 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000039773815",
                    "natureText": "DECRET",
                    "datePubliTexte": "2019-12-29",
