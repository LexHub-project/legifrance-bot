                {
                    "textCid": "JORFTEXT000026705231",
                    "textTitle": "D\u00e9cret n\u00b02012-1331\n du 29 novembre 2012 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000026706771",
                    "natureText": "DECRET",
