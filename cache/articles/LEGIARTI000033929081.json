                {
                    "textCid": "JORFTEXT000042364321",
                    "textTitle": "D\u00e9cret n\u00b02020-1168 du 24 septembre 2020 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000042367384",
                    "natureText": "DECRET",
                    "datePubliTexte": "2020-09-26",
