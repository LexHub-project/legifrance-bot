                {
                    "textCid": "JORFTEXT000025757625",
                    "textTitle": "D\u00e9cret n\u00b02012-597\n du 27 avril 2012 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
