                {
                    "textCid": "JORFTEXT000000825108",
                    "textTitle": "D\u00e9cret 2007-1467 2007-10-12 JORF 16 octobre 2007",
                    "linkType": "CODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "",
                    "articleId": "JORFTEXT000000825108",
                    "natureText": "DECRET",
                    "datePubliTexte": "2007-10-16",
                    "dateSignaTexte": "2007-10-12",
