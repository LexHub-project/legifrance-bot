                    "textCid": "JORFTEXT000038766567",
                    "textTitle": "LOI n\u00b02019-733 du 14 juillet 2019 - art. 2",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000038767231",
                    "natureText": "LOI",
                    "datePubliTexte": "2019-07-16",
