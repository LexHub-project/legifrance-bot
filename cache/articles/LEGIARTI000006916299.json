                {
                    "textCid": "JORFTEXT000030426783",
                    "textTitle": "D\u00c9CRET n\u00b02015-374 du 31 mars 2015 - art. 4",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "4",
