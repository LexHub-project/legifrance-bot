                {
                    "textCid": "JORFTEXT000043511987",
                    "textTitle": "D\u00e9cret n\u00b02021-613 du 18 mai 2021 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000043512888",
                    "natureText": "DECRET",
