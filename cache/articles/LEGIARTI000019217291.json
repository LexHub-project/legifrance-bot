                    "textCid": "JORFTEXT000019215060",
                    "textTitle": "Ordonnance n\u00b02008-717\n du 17 juillet 2008 - art. 11",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "11",
                    "articleId": "LEGIARTI000019216422",
                    "natureText": "ORDONNANCE",
                    "datePubliTexte": "2008-07-19",
