                {
                    "textCid": "JORFTEXT000041506557",
                    "textTitle": "Ordonnance n\u00b0 2020-71 du 29 janvier 2020 - art. 4",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "4",
                    "articleId": "LEGIARTI000041562499",
