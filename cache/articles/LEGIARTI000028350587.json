                {
                    "textCid": "JORFTEXT000046113837",
                    "textTitle": "Ordonnance n\u00b02022-1086 du 29 juillet 2022 - art. 13",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "13",
                    "articleId": "LEGIARTI000046118724",
                    "natureText": "ORDONNANCE",
