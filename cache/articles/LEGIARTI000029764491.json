                    "textCid": "JORFTEXT000029761800",
                    "textTitle": "D\u00c9CRET n\u00b02014-1359 du 14 novembre 2014 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000029763479",
                    "natureText": "DECRET",
