                {
                    "textCid": "JORFTEXT000031251368",
                    "textTitle": "D\u00c9CRET n\u00b02015-1201 du 29 septembre 2015 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
