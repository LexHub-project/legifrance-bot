                {
                    "textCid": "JORFTEXT000024562958",
                    "textTitle": "Ordonnance n\u00b02011-1105\n du 14 septembre 2011 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
