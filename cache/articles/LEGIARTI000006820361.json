                {
                    "textCid": "JORFTEXT000038365512",
                    "textTitle": "D\u00e9cret n\u00b02019-303 du 10 avril 2019 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000038366396",
                    "natureText": "DECRET",
