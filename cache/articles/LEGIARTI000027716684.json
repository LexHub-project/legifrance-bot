                    "textCid": "JORFTEXT000027713399",
                    "textTitle": "LOI n\u00b02013-619\n du 16 juillet 2013 - art. 14",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "14",
                    "articleId": "LEGIARTI000027715517",
                    "natureText": "LOI",
