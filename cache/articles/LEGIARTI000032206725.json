                {
                    "textCid": "JORFTEXT000045133771",
                    "textTitle": "LOI n\u00b02022-140 du 7 f\u00e9vrier 2022 - art. 38",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
