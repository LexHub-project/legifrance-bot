                {
                    "textCid": "JORFTEXT000031740543",
                    "textTitle": "Arr\u00eat\u00e9 du 30 d\u00e9cembre 2015 - art. 1",
                    "linkType": "DEPLACE",
                    "linkOrientation": "cible",
