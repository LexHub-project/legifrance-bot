                {
                    "textCid": "JORFTEXT000027617519",
                    "textTitle": "Ordonnance n\u00b02013-544\n du 27 juin 2013 - art. 11",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "11",
