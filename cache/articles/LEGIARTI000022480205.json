                {
                    "textCid": "JORFTEXT000025104850",
                    "textTitle": "Ordonnance n\u00b02012-6\n du 5 janvier 2012 - art. 1",
                    "linkType": "DEPLACE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000025107636",
