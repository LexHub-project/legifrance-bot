                {
                    "textCid": "JORFTEXT000025199018",
                    "textTitle": "D\u00e9cret n\u00b02012-83\n du 24 janvier 2012 - art. 9",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "9",
                    "articleId": "LEGIARTI000025199561",
                    "natureText": "DECRET",
