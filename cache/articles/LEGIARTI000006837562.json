                {
                    "textCid": "JORFTEXT000035168256",
                    "textTitle": "D\u00e9cret n\u00b02017-1156 du 10 juillet 2017 - art. 7",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
