                {
                    "textCid": "JORFTEXT000034378820",
                    "textTitle": "D\u00e9cret n\u00b02017-483 du 6 avril 2017 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
