                {
                    "textCid": "JORFTEXT000000459023",
                    "textTitle": "D\u00e9cret n\u00b02006-1099 du 31 ao\u00fbt 2006 - art. 2 () JORF 1er septembre 2006",
                    "linkType": "MODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000006254541",
                    "natureText": "DECRET",
                    "datePubliTexte": "2006-09-01",
