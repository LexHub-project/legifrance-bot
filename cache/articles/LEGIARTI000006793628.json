                {
                    "textCid": "JORFTEXT000044044441",
                    "textTitle": "Ordonnance n\u00b02021-1192 du 15 septembre 2021 - art. 34",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "34",
                    "articleId": "LEGIARTI000044045494",
                    "natureText": "ORDONNANCE",
