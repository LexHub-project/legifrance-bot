                {
                    "textCid": "JORFTEXT000045197395",
                    "textTitle": "LOI n\u00b02022-217 du 21 f\u00e9vrier 2022 - art. 191",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "191",
