                {
                    "textCid": "JORFTEXT000037277327",
                    "textTitle": "D\u00e9cret n\u00b02018-686 du 1er ao\u00fbt 2018 - art. 5",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
