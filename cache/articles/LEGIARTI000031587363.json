                    "textCid": "JORFTEXT000031585569",
                    "textTitle": "D\u00e9cret n\u00b02015-1606 du 7 d\u00e9cembre 2015 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000031586594",
                    "natureText": "DECRET",
