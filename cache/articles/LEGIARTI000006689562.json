                {
                    "textCid": "JORFTEXT000028683576",
                    "textTitle": "LOI n\u00b02014-288\n du 5 mars 2014 - art. 21 (VD)",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "21",
