                {
                    "textCid": "JORFTEXT000022424491",
                    "textTitle": "D\u00e9cret n\u00b02010-750 du 2 juillet 2010 - art. 1",
                    "linkType": "TRANSFERE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
