                {
                    "textCid": "JORFTEXT000025062356",
                    "textTitle": "D\u00e9cret n\u00b02011-2113 du 30 d\u00e9cembre 2011 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000025101208",
                    "natureText": "DECRET",
