                {
                    "textCid": "JORFTEXT000033616372",
                    "textTitle": "D\u00e9cret n\u00b02016-1726 du 14 d\u00e9cembre 2016 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000033618211",
                    "natureText": "DECRET",
