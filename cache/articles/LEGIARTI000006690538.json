                {
                    "textCid": "JORFTEXT000028652182",
                    "textTitle": "LOI n\u00b02014-201\n du 24 f\u00e9vrier 2014 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
