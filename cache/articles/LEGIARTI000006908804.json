                {
                    "textCid": "JORFTEXT000034492649",
                    "textTitle": "D\u00e9cret n\u00b02017-631 du 25 avril 2017 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000034494047",
