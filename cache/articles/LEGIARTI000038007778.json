                {
                    "textCid": "JORFTEXT000045946291",
                    "textTitle": "D\u00e9cret n\u00b02022-917 du 21 juin 2022 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000045947156",
