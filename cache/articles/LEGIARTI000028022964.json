                {
                    "textCid": "JORFTEXT000037213169",
                    "textTitle": "D\u00e9cret n\u00b02018-617 du 17 juillet 2018 - art. 7",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "7",
