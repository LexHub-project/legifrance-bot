                {
                    "textCid": "JORFTEXT000044638030",
                    "textTitle": "D\u00e9cret n\u00b02021-1905 du 30 d\u00e9cembre 2021 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
