                {
                    "textCid": "JORFTEXT000044472224",
                    "textTitle": "D\u00e9cret n\u00b02021-1627 du 10 d\u00e9cembre 2021 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
