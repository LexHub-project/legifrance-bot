                {
                    "textCid": "JORFTEXT000021092948",
                    "textTitle": "D\u00e9cret n\u00b02009-1162\n du 30 septembre 2009 - art. 4",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "4",
                    "articleId": "LEGIARTI000021093639",
                    "natureText": "DECRET",
                    "datePubliTexte": "2009-10-02",
