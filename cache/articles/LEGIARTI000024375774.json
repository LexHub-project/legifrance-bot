                    "textCid": "JORFTEXT000024374063",
                    "textTitle": "D\u00e9cret n\u00b02011-847\n du 18 juillet 2011 - art. 6",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "6",
                    "articleId": "LEGIARTI000024374591",
                    "natureText": "DECRET",
