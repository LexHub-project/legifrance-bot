                {
                    "textCid": "JORFTEXT000032584137",
                    "textTitle": "D\u00e9cret n\u00b02016-667 du 24 mai 2016 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000032585470",
                    "natureText": "DECRET",
