                {
                    "textCid": "JORFTEXT000000516442",
                    "textTitle": "D\u00e9cret 78-621 1978-05-31 JORF 8 juin 1978",
                    "linkType": "CODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "",
                    "articleId": "JORFTEXT000000516442",
                    "natureText": "DECRET",
