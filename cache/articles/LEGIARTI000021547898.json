                {
                    "textCid": "JORFTEXT000030171150",
                    "textTitle": "D\u00c9CRET n\u00b02015-87 du 28 janvier 2015 - art. 1",
                    "linkType": "DEPLACE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
