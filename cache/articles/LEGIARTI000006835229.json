                {
                    "textCid": "JORFTEXT000024357948",
                    "textTitle": "D\u00e9cret n\u00b02011-832\n du 12 juillet 2011 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000024359661",
                    "natureText": "DECRET",
