                {
                    "textCid": "JORFTEXT000045441846",
                    "textTitle": "Ordonnance n\u00b02022-448 du 30 mars 2022 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000045443437",
                    "natureText": "ORDONNANCE",
