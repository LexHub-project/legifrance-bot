                {
                    "textCid": "JORFTEXT000030370308",
                    "textTitle": "D\u00c9CRET n\u00b02015-301 du 17 mars 2015 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
