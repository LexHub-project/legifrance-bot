                {
                    "textCid": "JORFTEXT000022512406",
                    "textTitle": "D\u00e9cret n\u00b02010-862\n du 23 juillet 2010 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
