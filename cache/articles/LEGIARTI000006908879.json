                {
                    "textCid": "JORFTEXT000029449080",
                    "textTitle": "D\u00c9CRET n\u00b02014-1042 du 12 septembre 2014 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000029450345",
                    "natureText": "DECRET",
