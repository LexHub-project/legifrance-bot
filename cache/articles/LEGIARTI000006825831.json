                {
                    "textCid": "JORFTEXT000037639478",
                    "textTitle": "LOI n\u00b02018-1021 du 23 novembre 2018 - art. 109 (V)",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "109",
