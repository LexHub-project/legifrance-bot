                {
                    "textCid": "JORFTEXT000041768384",
                    "textTitle": "D\u00e9cret n\u00b02020-372 du 30 mars 2020 - art. 10",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "10",
