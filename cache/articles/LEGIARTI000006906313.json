                {
                    "textCid": "JORFTEXT000020480797",
                    "textTitle": "D\u00e9cret n\u00b02009-378\n du 2 avril 2009 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
