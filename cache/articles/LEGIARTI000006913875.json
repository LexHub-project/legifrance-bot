                {
                    "textCid": "JORFTEXT000034166895",
                    "textTitle": "D\u00e9cret n\u00b02017-319 du 10 mars 2017 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
