                {
                    "textCid": "JORFTEXT000028572356",
                    "textTitle": "D\u00e9cret n\u00b02014-114 du 7 f\u00e9vrier 2014 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
