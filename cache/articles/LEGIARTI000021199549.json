                {
                    "textCid": "JORFTEXT000029762609",
                    "textTitle": "D\u00c9CRET n\u00b02014-1367 du 14 novembre 2014 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
