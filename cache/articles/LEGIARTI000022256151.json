                    "textCid": "JORFTEXT000022244239",
                    "textTitle": "D\u00e9cret n\u00b02010-526\n du 20 mai 2010 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000022255940",
                    "natureText": "DECRET",
