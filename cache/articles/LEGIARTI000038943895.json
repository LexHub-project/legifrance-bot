                    "textCid": "JORFTEXT000038939847",
                    "textTitle": "D\u00e9cret n\u00b02019-850 du 20 ao\u00fbt 2019 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000038942270",
                    "natureText": "DECRET",
                    "datePubliTexte": "2019-08-22",
