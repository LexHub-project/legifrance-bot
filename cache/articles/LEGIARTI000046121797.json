                    "textCid": "JORFTEXT000046113837",
                    "textTitle": "Ordonnance n\u00b02022-1086 du 29 juillet 2022 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000046118710",
                    "natureText": "ORDONNANCE",
