                    "textCid": "JORFTEXT000046014410",
                    "textTitle": "D\u00e9lib\u00e9ration n\u00b02022/CA/12 du 30 juin 2022 - art. 6",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "6",
                    "articleId": "LEGIARTI000046017537",
                    "natureText": "DELIBERATION",
