                {
                    "textCid": "JORFTEXT000024374011",
                    "textTitle": "D\u00e9cret n\u00b02011-846\n du 18 juillet 2011 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000024374550",
                    "natureText": "DECRET",
