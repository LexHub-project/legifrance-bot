                {
                    "textCid": "JORFTEXT000025836786",
                    "textTitle": "D\u00e9cret n\u00b02012-741\n du 9 mai 2012 - art. 10",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "10",
                    "articleId": "LEGIARTI000025849193",
                    "natureText": "DECRET",
                    "datePubliTexte": "2012-05-10",
