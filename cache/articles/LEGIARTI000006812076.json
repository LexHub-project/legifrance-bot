                {
                    "textCid": "JORFTEXT000039110244",
                    "textTitle": "D\u00e9cret n\u00b02019-966 du 18 septembre 2019 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
