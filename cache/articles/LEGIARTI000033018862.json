                {
                    "textCid": "JORFTEXT000036240557",
                    "textTitle": "Ordonnance n\u00b02017-1718 du 20 d\u00e9cembre 2017 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
