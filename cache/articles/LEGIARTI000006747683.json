                {
                    "textCid": "JORFTEXT000042266405",
                    "textTitle": "D\u00e9cret n\u00b02020-1090 du 25 ao\u00fbt 2020 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
