                {
                    "textCid": "JORFTEXT000043339548",
                    "textTitle": "Ordonnance n\u00b02021-409 du 8 avril 2021 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000043341153",
                    "natureText": "ORDONNANCE",
