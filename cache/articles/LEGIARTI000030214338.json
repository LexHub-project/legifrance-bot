                {
                    "textCid": "JORFTEXT000031912641",
                    "textTitle": "LOI n\u00b02016-41 du 26 janvier 2016 - art. 61",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "61",
                    "articleId": "LEGIARTI000031916490",
