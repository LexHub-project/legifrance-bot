                {
                    "textCid": "JORFTEXT000033420866",
                    "textTitle": "D\u00e9cret n\u00b02016-1553 du 18 novembre 2016 - art. 7",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "7",
                    "articleId": "LEGIARTI000033464155",
                    "natureText": "DECRET",
                    "datePubliTexte": "2016-11-19",
