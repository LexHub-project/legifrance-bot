                {
                    "textCid": "JORFTEXT000031353863",
                    "textTitle": "ORDONNANCE n\u00b02015-1324 du 22 octobre 2015 - art. 5",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "5",
                    "articleId": "LEGIARTI000031355082",
                    "natureText": "ORDONNANCE",
                    "datePubliTexte": "2015-10-23",
