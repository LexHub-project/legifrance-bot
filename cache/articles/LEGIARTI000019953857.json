                {
                    "textCid": "JORFTEXT000019944338",
                    "textTitle": "D\u00e9cret n\u00b02008-1338 du 16 d\u00e9cembre 2008 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
