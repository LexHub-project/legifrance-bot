                {
                    "textCid": "JORFTEXT000032471614",
                    "textTitle": "D\u00e9cret n\u00b02016-530 du 27 avril 2016 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
