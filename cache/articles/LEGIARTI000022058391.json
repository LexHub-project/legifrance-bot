                {
                    "textCid": "JORFTEXT000044806946",
                    "textTitle": "D\u00e9cret n\u00b02021-1954 du 31 d\u00e9cembre 2021 - art. 1",
                    "linkType": "ABROGE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000044940303",
                    "natureText": "DECRET",
