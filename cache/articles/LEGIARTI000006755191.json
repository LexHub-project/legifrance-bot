                {
                    "textCid": "JORFTEXT000036267297",
                    "textTitle": "D\u00e9cret n\u00b02017-1765 du 26 d\u00e9cembre 2017 - art. 4",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "4",
