                {
                    "textCid": "JORFTEXT000032003979",
                    "textTitle": "Ordonnance n\u00b02016-128 du 10 f\u00e9vrier 2016 - art. 19",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "19",
                    "articleId": "LEGIARTI000032005967",
                    "natureText": "ORDONNANCE",
