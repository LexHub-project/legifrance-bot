                {
                    "textCid": "JORFTEXT000044202140",
                    "textTitle": "Ordonnance n\u00b02021-1330 du 13 octobre 2021 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000044202873",
                    "natureText": "ORDONNANCE",
