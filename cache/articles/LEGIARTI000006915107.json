                {
                    "textCid": "JORFTEXT000026588856",
                    "textTitle": "D\u00e9cret n\u00b02012-1236\n du 6 novembre 2012 - art. 5",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "5",
                    "articleId": "LEGIARTI000026589558",
                    "natureText": "DECRET",
