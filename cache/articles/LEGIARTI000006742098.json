                {
                    "textCid": "JORFTEXT000000509071",
                    "textTitle": "Loi n\u00b088-16 du 5 janvier 1988 - art. 1 (V) JORF 6 janvier 1988",
                    "linkType": "MODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000006757215",
                    "natureText": "LOI",
                    "datePubliTexte": "1988-01-06",
