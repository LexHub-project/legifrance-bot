                {
                    "textCid": "JORFTEXT000033623473",
                    "textTitle": "D\u00e9cret n\u00b02016-1744 du 15 d\u00e9cembre 2016 - art. 3",
                    "linkType": "DEPLACE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
