                {
                    "textCid": "JORFTEXT000039666574",
                    "textTitle": "LOI n\u00b02019-1428 du 24 d\u00e9cembre 2019 - art. 158 (V)",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "158",
                    "articleId": "LEGIARTI000039668887",
                    "natureText": "LOI",
                    "datePubliTexte": "2019-12-26",
                    "dateSignaTexte": "2019-12-24",
