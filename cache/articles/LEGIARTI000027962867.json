                {
                    "textCid": "JORFTEXT000030802747",
                    "textTitle": "D\u00c9CRET n\u00b02015-770 du 29 juin 2015 - art. 1",
                    "linkType": "DEPLACE",
                    "linkOrientation": "cible",
