                {
                    "textCid": "JORFTEXT000025836786",
                    "textTitle": "D\u00e9cret n\u00b02012-741\n du 9 mai 2012 - art. 5",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "5",
