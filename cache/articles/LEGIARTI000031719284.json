                {
                    "textCid": "JORFTEXT000046070341",
                    "textTitle": "D\u00e9cret n\u00b02022-1025 du 20 juillet 2022 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000046071264",
