                {
                    "textCid": "JORFTEXT000034581861",
                    "textTitle": "Ordonnance n\u00b02017-762 du 4 mai 2017 - art. 36",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "36",
                    "articleId": "LEGIARTI000034583088",
                    "natureText": "ORDONNANCE",
