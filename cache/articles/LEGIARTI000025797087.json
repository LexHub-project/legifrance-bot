                {
                    "textCid": "JORFTEXT000025794106",
                    "textTitle": "D\u00e9cret n\u00b02012-615\n du 2 mai 2012 - art. 3",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000025795429",
                    "natureText": "DECRET",
                    "datePubliTexte": "2012-05-04",
