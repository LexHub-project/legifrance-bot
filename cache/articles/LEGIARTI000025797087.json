                {
                    "textCid": "JORFTEXT000036012202",
                    "textTitle": "D\u00e9cret n\u00b02017-1557 du 10 novembre 2017 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000036013522",
                    "natureText": "DECRET",
