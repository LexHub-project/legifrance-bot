                {
                    "textCid": "JORFTEXT000031700731",
                    "textTitle": "LOI n\u00b02015-1776 du 28 d\u00e9cembre 2015 - art. 32",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "32",
