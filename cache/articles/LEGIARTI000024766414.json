                {
                    "textCid": "JORFTEXT000031585569",
                    "textTitle": "D\u00e9cret n\u00b02015-1606 du 7 d\u00e9cembre 2015 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
