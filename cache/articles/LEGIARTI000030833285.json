                {
                    "textCid": "JORFTEXT000043876194",
                    "textTitle": "D\u00e9cret n\u00b02021-1000 du 30 juillet 2021 - art. 6",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "6",
                    "articleId": "LEGIARTI000043882038",
                    "natureText": "DECRET",
