                {
                    "textCid": "JORFTEXT000041712000",
                    "textTitle": "Ordonnance n\u00b0 2020-232 du 11 mars 2020 - art. 20",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "20",
                    "articleId": "LEGIARTI000041713845",
                    "natureText": "ORDONNANCE",
