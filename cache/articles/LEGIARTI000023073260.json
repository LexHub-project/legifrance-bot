                {
                    "textCid": "JORFTEXT000043512041",
                    "textTitle": "Ordonnance n\u00b02021-614 du 19 mai 2021 - art. 27",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "27",
                    "articleId": "LEGIARTI000043513118",
