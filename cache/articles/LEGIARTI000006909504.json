                {
                    "textCid": "JORFTEXT000022041034",
                    "textTitle": "D\u00e9cret n\u00b02010-344\n du 31 mars 2010 - art. 33",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "33",
                    "articleId": "LEGIARTI000022043573",
                    "natureText": "DECRET",
                    "datePubliTexte": "2010-04-01",
