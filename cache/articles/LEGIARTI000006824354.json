                {
                    "textCid": "JORFTEXT000041506557",
                    "textTitle": "Ordonnance n\u00b0 2020-71 du 29 janvier 2020 - art.",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "",
                    "articleId": "LEGIARTI000041562511",
                    "natureText": "ORDONNANCE",
                    "datePubliTexte": "2020-01-31",
