                {
                    "textCid": "JORFTEXT000029926655",
                    "textTitle": "LOI n\u00b02014-1545 du 20 d\u00e9cembre 2014 - art. 38",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
