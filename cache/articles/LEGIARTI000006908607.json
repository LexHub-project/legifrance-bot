                {
                    "textCid": "JORFTEXT000035448324",
                    "textTitle": "D\u00e9cret n\u00b02017-1288 du 21 ao\u00fbt 2017 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000035448897",
                    "natureText": "DECRET",
