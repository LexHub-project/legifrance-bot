                {
                    "textCid": "JORFTEXT000036756145",
                    "textTitle": "D\u00e9cret n\u00b02018-229 du 30 mars 2018 - art. 7",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "7",
                    "articleId": "LEGIARTI000036760028",
                    "natureText": "DECRET",
