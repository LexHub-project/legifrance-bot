                {
                    "textCid": "JORFTEXT000029965972",
                    "textTitle": "D\u00c9CRET n\u00b02014-1638 du 26 d\u00e9cembre 2014 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
