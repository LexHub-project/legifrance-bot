                    "textCid": "JORFTEXT000022470434",
                    "textTitle": "LOI n\u00b0 2010-788\n du 12 juillet 2010 - art. 216",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "216",
                    "articleId": "LEGIARTI000022473228",
                    "natureText": "LOI",
                    "datePubliTexte": "2010-07-13",
