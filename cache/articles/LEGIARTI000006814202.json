                {
                    "textCid": "JORFTEXT000031638556",
                    "textTitle": "D\u00e9cret n\u00b02015-1689 du 17 d\u00e9cembre 2015 - art. 4",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "4",
