                {
                    "textCid": "JORFTEXT000025053440",
                    "textTitle": "LOI n\u00b02011-2012\n du 29 d\u00e9cembre 2011 - art. 5",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "5",
