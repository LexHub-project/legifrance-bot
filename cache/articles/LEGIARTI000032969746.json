                    "textCid": "JORFTEXT000032966914",
                    "textTitle": "Ordonnance n\u00b02016-1060 du 3 ao\u00fbt 2016 - art. 2",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000032968880",
                    "natureText": "ORDONNANCE",
