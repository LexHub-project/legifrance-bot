                {
                    "textCid": "JORFTEXT000000515331",
                    "textTitle": "D\u00e9cret 77-38 1977-01-03 JORF 18 JANVIER 1977",
                    "linkType": "CODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "",
