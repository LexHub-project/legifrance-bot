                {
                    "textCid": "JORFTEXT000032471614",
                    "textTitle": "D\u00e9cret n\u00b02016-530 du 27 avril 2016 - art. 2",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
                    "articleId": "LEGIARTI000032473363",
                    "natureText": "DECRET",
                    "datePubliTexte": "2016-04-30",
