                {
                    "textCid": "JORFTEXT000042665307",
                    "textTitle": "LOI n\u00b02020-1576 du 14 d\u00e9cembre 2020 - art. 100 (V)",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "100",
