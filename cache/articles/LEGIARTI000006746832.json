                {
                    "textCid": "JORFTEXT000026355185",
                    "textTitle": "D\u00e9cret n\u00b02012-1032\n du 7 septembre 2012 - art. 1",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
                    "articleId": "LEGIARTI000026356962",
                    "natureText": "DECRET",
                    "datePubliTexte": "2012-09-09",
                    "dateSignaTexte": "2012-09-07",
