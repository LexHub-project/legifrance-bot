                {
                    "textCid": "JORFTEXT000046113609",
                    "textTitle": "Ordonnance n\u00b02022-1076 du 29 juillet 2022 - art. 3",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "3",
                    "articleId": "LEGIARTI000046118253",
                    "natureText": "ORDONNANCE",
