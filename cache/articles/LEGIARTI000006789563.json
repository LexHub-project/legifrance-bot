                {
                    "textCid": "JORFTEXT000036750837",
                    "textTitle": "Arr\u00eat\u00e9 du 27 mars 2018 - art. 17",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "17",
