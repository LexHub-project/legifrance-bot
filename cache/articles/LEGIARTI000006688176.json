                {
                    "textCid": "JORFTEXT000000615568",
                    "textTitle": "Loi n\u00b02007-297 du 5 mars 2007 - art. 48 () JORF 7 mars 2007",
                    "linkType": "MODIFICATION",
                    "linkOrientation": "source",
                    "articleNum": "48",
