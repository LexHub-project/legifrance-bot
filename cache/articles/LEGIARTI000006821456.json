                {
                    "textCid": "JORFTEXT000042574191",
                    "textTitle": "D\u00e9cret n\u00b02020-1452 du 27 novembre 2020 - art. 7",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "7",
