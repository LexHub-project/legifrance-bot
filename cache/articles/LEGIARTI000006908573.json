                {
                    "textCid": "JORFTEXT000025431170",
                    "textTitle": "D\u00e9cret n\u00b02012-298\n du 2 mars 2012 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
