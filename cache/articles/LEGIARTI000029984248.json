                {
                    "textCid": "JORFTEXT000029966013",
                    "textTitle": "D\u00c9CRET n\u00b02014-1641 du 26 d\u00e9cembre 2014 - art. 1",
                    "linkType": "CREE",
                    "linkOrientation": "cible",
                    "articleNum": "1",
