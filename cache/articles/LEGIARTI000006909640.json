                {
                    "textCid": "JORFTEXT000042283930",
                    "textTitle": "D\u00e9cret n\u00b02020-1094 du 27 ao\u00fbt 2020 - art. 2",
                    "linkType": "MODIFIE",
                    "linkOrientation": "cible",
                    "articleNum": "2",
